{"ast":null,"code":"import HeaderCont from \"@/components/HeaderCont.vue\";\nimport FooterCont from \"@/components/FooterCont.vue\";\nimport TitleCont from \"@/components/TitleCont.vue\";\nimport ContactCont from \"@/components/ContactCont.vue\";\nimport { ref } from \"vue\";\nimport { Swiper, SwiperSlide } from \"swiper/vue\";\nimport \"swiper/css\";\nimport \"swiper/css/effect-cards\";\nimport { EffectCards } from \"swiper\";\nexport default {\n  components: {\n    HeaderCont,\n    FooterCont,\n    TitleCont,\n    ContactCont,\n    Swiper,\n    SwiperSlide\n  },\n  data: function () {\n    return {\n      modules: [EffectCards],\n      titles: [{\n        name: \"3D Renders\"\n      }, {\n        name: \"Experimental\"\n      }, {\n        name: \"Architecture\"\n      }, {\n        name: \"Fashion\"\n      }, {\n        name: \"Nature\"\n      }, {\n        name: \"People\"\n      }, {\n        name: \"Athletics\"\n      }]\n    };\n  },\n  setup() {\n    const splashes = ref([]);\n    const sliders = ref([]);\n    const search = ref(\"landscape\");\n\n    // const SearchSplashes = () => {\n    //   fetch(\n    //     `https://api.unsplash.com/search/photos/?client_id=ArGvdKlJWl3YpwcaZmGDf5jEEVo81-0sbA8evCVSBo4&query=${search.value}`\n    //   )\n    //     .then((response) => response.json())\n    //     .then((result) => console.log(result))\n    //     .then((error) => console.log(error));\n    // };\n    // SearchSplashes()\n    const RandomSplashes = () => {\n      fetch(\"https://api.unsplash.com/photos/random/?client_id=ArGvdKlJWl3YpwcaZmGDf5jEEVo81-0sbA8evCVSBo4&count=30\").then(response => response.json()).then(result => splashes.value = result)\n      // .then((result) => console.log(result))\n      .then(error => console.log(error));\n    };\n    RandomSplashes();\n    const TopSplashes = () => {\n      fetch(\"https://api.unsplash.com/photos/random/?client_id=ArGvdKlJWl3YpwcaZmGDf5jEEVo81-0sbA8evCVSBo4&count=10\").then(response => response.json()).then(result => sliders.value = result).then(error => console.log(error));\n    };\n    TopSplashes();\n    const SearchUnsplashes = async () => {\n      fetch(`https://api.unsplash.com/search/photos?client_id=ArGvdKlJWl3YpwcaZmGDf5jEEVo81-0sbA8evCVSBo4&query=${search.value}`).then(response => response.json()).then(result => {\n        splashes.value = result.results;\n        search.value = \"\";\n      }).catch(error => console.log(error));\n    };\n    SearchUnsplashes();\n    return {\n      splashes,\n      search,\n      sliders,\n      RandomSplashes,\n      TopSplashes,\n      SearchUnsplashes\n    };\n  }\n};","map":{"version":3,"mappings":"AA4DA,OAAOA,UAAS,MAAO,6BAA6B;AACpD,OAAOC,UAAS,MAAO,6BAA6B;AACpD,OAAOC,SAAQ,MAAO,4BAA4B;AAClD,OAAOC,WAAU,MAAO,8BAA8B;AACtD,SAASC,GAAE,QAAS,KAAK;AAEzB,SAASC,MAAM,EAAEC,WAAU,QAAS,YAAY;AAChD,OAAO,YAAY;AACnB,OAAO,yBAAyB;AAChC,SAASC,WAAU,QAAS,QAAQ;AAEpC,eAAe;EACbC,UAAU,EAAE;IACVR,UAAU;IACVC,UAAU;IACVC,SAAS;IACTC,WAAW;IACXE,MAAM;IACNC;EACF,CAAC;EAEDG,IAAI,EAAE,YAAY;IAChB,OAAO;MACLC,OAAO,EAAE,CAACH,WAAW,CAAC;MACtBI,MAAM,EAAE,CACN;QAAEC,IAAI,EAAE;MAAa,CAAC,EACtB;QAAEA,IAAI,EAAE;MAAe,CAAC,EACxB;QAAEA,IAAI,EAAE;MAAe,CAAC,EACxB;QAAEA,IAAI,EAAE;MAAU,CAAC,EACnB;QAAEA,IAAI,EAAE;MAAS,CAAC,EAClB;QAAEA,IAAI,EAAE;MAAS,CAAC,EAClB;QAAEA,IAAI,EAAE;MAAY,CAAC;IAEzB,CAAC;EACH,CAAC;EAEDC,KAAK,GAAG;IACN,MAAMC,QAAO,GAAIV,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMW,OAAM,GAAIX,GAAG,CAAC,EAAE,CAAC;IACvB,MAAMY,MAAK,GAAIZ,GAAG,CAAC,WAAW,CAAC;;IAE/B;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA,MAAMa,cAAa,GAAI,MAAM;MAC3BC,KAAK,CACH,wGAAuG,CACzG,CACGC,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACC,IAAI,EAAE,EAClCF,IAAI,CAAEG,MAAM,IAAMR,QAAQ,CAACS,KAAI,GAAID,MAAO;MAC3C;MAAA,CACCH,IAAI,CAAEK,KAAK,IAAKC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC,CAAC;IACxC,CAAC;IACDP,cAAc,EAAE;IAEhB,MAAMU,WAAU,GAAI,MAAM;MACxBT,KAAK,CACH,wGAAuG,CACzG,CACGC,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACC,IAAI,EAAE,EAClCF,IAAI,CAAEG,MAAM,IAAMP,OAAO,CAACQ,KAAI,GAAID,MAAO,EACzCH,IAAI,CAAEK,KAAK,IAAKC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC,CAAC;IACxC,CAAC;IACDG,WAAW,EAAE;IAEb,MAAMC,gBAAe,GAAI,YAAY;MACnCV,KAAK,CACF,sGAAqGF,MAAM,CAACO,KAAM,GACrH,CACGJ,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACC,IAAI,EAAE,EAClCF,IAAI,CAAEG,MAAM,IAAK;QAChBR,QAAQ,CAACS,KAAI,GAAID,MAAM,CAACO,OAAO;QAC/Bb,MAAM,CAACO,KAAI,GAAI,EAAE;MACnB,CAAC,EACAO,KAAK,CAAEN,KAAK,IAAKC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC,CAAC;IACzC,CAAC;IACDI,gBAAgB,EAAE;IAElB,OAAO;MACLd,QAAQ;MACRE,MAAM;MACND,OAAO;MACPE,cAAc;MACdU,WAAW;MACXC;IACF,CAAC;EACH;AACF,CAAC","names":["HeaderCont","FooterCont","TitleCont","ContactCont","ref","Swiper","SwiperSlide","EffectCards","components","data","modules","titles","name","setup","splashes","sliders","search","RandomSplashes","fetch","then","response","json","result","value","error","console","log","TopSplashes","SearchUnsplashes","results","catch"],"sourceRoot":"","sources":["C:\\Users\\chanm\\Documents\\vue_api\\src\\views\\UnsplashView.vue"],"sourcesContent":["<template>\n  <div>\n    <HeaderCont />\n    <TitleCont name1=\"unsplash\" name2=\"reference api\" />\n    <section class=\"unsplash__wrap\">\n      <div class=\"container\">\n        <div class=\"unsplash__inner\">\n          <h2>Unsplash Random</h2>\n          <ul>\n            <li>\n              <swiper\n                :effect=\"'cards'\"\n                :grabCursor=\"true\"\n                :modules=\"modules\"\n                class=\"mySwiper\"\n              >\n                <swiper-slide v-for=\"slide in sliders\" :key=\"slide.id\">\n                  <a :href=\"`https://unsplash.com/photos/${slide.id}`\">\n                    <img :src=\"slide.urls.regular\" :alt=\"slide.id\" />\n                  </a>\n                </swiper-slide>\n              </swiper>\n            </li>\n          </ul>\n        </div>\n        <div class=\"btn__list\">\n          <button v-for=\"title in titles\" v-bind:key=\"title.name\">\n            {{ title.name }}\n          </button>\n        </div>\n        <div class=\"unsplash_search\">\n          <h2>검색하기</h2>\n          <form @submit.prevent=\"SearchUnsplashes()\">\n            <input\n              type=\"search\"\n              id=\"search\"\n              placeholder=\"검색하세요\"\n              v-model=\"search\"\n            />\n            <button type=\"submit\">검색</button>\n          </form>\n        </div>\n        <div class=\"cont__unsplash\">\n          <ul>\n            <li v-for=\"splash in splashes\" :key=\"splash.id\">\n              <a :href=\"`https://unsplash.com/photos/${splash.id}`\">\n                <img :src=\"splash.urls.regular\" :alt=\"splash.id\" />\n              </a>\n            </li>\n          </ul>\n        </div>\n      </div>\n    </section>\n\n    <ContactCont />\n    <FooterCont />\n  </div>\n</template>\n\n<script>\nimport HeaderCont from \"@/components/HeaderCont.vue\";\nimport FooterCont from \"@/components/FooterCont.vue\";\nimport TitleCont from \"@/components/TitleCont.vue\";\nimport ContactCont from \"@/components/ContactCont.vue\";\nimport { ref } from \"vue\";\n\nimport { Swiper, SwiperSlide } from \"swiper/vue\";\nimport \"swiper/css\";\nimport \"swiper/css/effect-cards\";\nimport { EffectCards } from \"swiper\";\n\nexport default {\n  components: {\n    HeaderCont,\n    FooterCont,\n    TitleCont,\n    ContactCont,\n    Swiper,\n    SwiperSlide,\n  },\n\n  data: function () {\n    return {\n      modules: [EffectCards],\n      titles: [\n        { name: \"3D Renders\" },\n        { name: \"Experimental\" },\n        { name: \"Architecture\" },\n        { name: \"Fashion\" },\n        { name: \"Nature\" },\n        { name: \"People\" },\n        { name: \"Athletics\" },\n      ],\n    };\n  },\n\n  setup() {\n    const splashes = ref([]);\n    const sliders = ref([]);\n    const search = ref(\"landscape\");\n\n    // const SearchSplashes = () => {\n    //   fetch(\n    //     `https://api.unsplash.com/search/photos/?client_id=ArGvdKlJWl3YpwcaZmGDf5jEEVo81-0sbA8evCVSBo4&query=${search.value}`\n    //   )\n    //     .then((response) => response.json())\n    //     .then((result) => console.log(result))\n    //     .then((error) => console.log(error));\n    // };\n    // SearchSplashes()\n    const RandomSplashes = () => {\n      fetch(\n        \"https://api.unsplash.com/photos/random/?client_id=ArGvdKlJWl3YpwcaZmGDf5jEEVo81-0sbA8evCVSBo4&count=30\"\n      )\n        .then((response) => response.json())\n        .then((result) => (splashes.value = result))\n        // .then((result) => console.log(result))\n        .then((error) => console.log(error));\n    };\n    RandomSplashes();\n\n    const TopSplashes = () => {\n      fetch(\n        \"https://api.unsplash.com/photos/random/?client_id=ArGvdKlJWl3YpwcaZmGDf5jEEVo81-0sbA8evCVSBo4&count=10\"\n      )\n        .then((response) => response.json())\n        .then((result) => (sliders.value = result))\n        .then((error) => console.log(error));\n    };\n    TopSplashes();\n\n    const SearchUnsplashes = async () => {\n      fetch(\n        `https://api.unsplash.com/search/photos?client_id=ArGvdKlJWl3YpwcaZmGDf5jEEVo81-0sbA8evCVSBo4&query=${search.value}`\n      )\n        .then((response) => response.json())\n        .then((result) => {\n          splashes.value = result.results;\n          search.value = \"\";\n        })\n        .catch((error) => console.log(error));\n    };\n    SearchUnsplashes();\n\n    return {\n      splashes,\n      search,\n      sliders,\n      RandomSplashes,\n      TopSplashes,\n      SearchUnsplashes,\n    };\n  },\n};\n</script>\n<style lang=\"scss\">\nbody {\n  overflow-x: hidden;\n}\n.cont__unsplash {\n  ul {\n    column-count: 4;\n    column-gap: 20px;\n    width: 100%;\n\n    li {\n      margin-bottom: 20px;\n\n      img {\n        border-radius: 5px;\n      }\n    }\n  }\n}\n\n.unsplash__inner {\n  margin-bottom: 70px;\n  h2 {\n    font-size: 40px;\n    color: var(--black);\n    margin-bottom: 70px;\n  }\n  .swiper-slide img {\n    box-shadow: 0px 20px 10px -10px #000;\n    width: 100%;\n  }\n\n  .swiper-slide {\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    border-radius: 18px;\n    font-size: 22px;\n    font-weight: bold;\n    color: #fff;\n  }\n\n  .swiper-3d .swiper-slide-shadow {\n    // background: #f0eeeb;\n  }\n\n  a {\n    color: var(--white);\n  }\n}\n.unsplash_search {\n  margin-bottom: 100px;\n  position: relative;\n\n  h2 {\n    color: var(--black);\n    font-size: 40px;\n    text-indent: -9999px;\n    height: 0;\n  }\n\n  input {\n    background: #d5d5d5;\n    border: 1px solid #d5d5d5;\n    border-radius: 50px;\n    color: var(--black);\n    width: 100%;\n    padding: 14px 30px;\n    font-family: var(--font-kor1);\n  }\n  button {\n    position: absolute;\n    right: 8px;\n    top: 5px;\n    width: 40px;\n    height: 40px;\n    border-radius: 50%;\n    border: 0;\n    font-family: var(--font-kor1);\n    cursor: pointer;\n    z-index: 1000;\n    color: #000;\n  }\n}\n.btn__list {\n  margin-top: -40px;\n  margin-bottom: 60px;\n  text-align: center;\n\n  button {\n    display: inline-block;\n    padding: 10px;\n    margin-right: 10px;\n    border-radius: 20px;\n    cursor: pointer;\n    background: #000;\n    color: #fff;\n    border: 1px solid #fff;\n  }\n  button:hover {\n    background: #fff;\n    color: #000;\n    transition: all 0.3s;\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}