{"ast":null,"code":"import HeaderCont from \"@/components/HeaderCont.vue\";\nimport FooterCont from \"@/components/FooterCont.vue\";\nimport TitleCont from \"@/components/TitleCont.vue\";\nimport ContactCont from \"@/components/ContactCont.vue\";\nimport { ref } from \"vue\";\nimport { Swiper, SwiperSlide } from \"swiper/vue\";\nimport \"swiper/css\";\nimport \"swiper/css/effect-creative\";\nimport { EffectCreative } from \"swiper\";\nexport default {\n  components: {\n    HeaderCont,\n    FooterCont,\n    TitleCont,\n    ContactCont,\n    Swiper,\n    SwiperSlide\n  },\n  setup() {\n    const youtubes = ref([]);\n    const sliders = ref([]);\n    const search = ref(\"landscape\");\n    const RandomYoutube = () => {\n      fetch(\"https://raw.githubusercontent.com/Kim-chanmi/react_api/main/src/utils/youtube_video.json\").then(response => response.json()).then(result => youtubes.value = result.items)\n      // .then((result) => console.log(result))\n      .catch(error => console.log(\"error\", error));\n    };\n    RandomYoutube();\n    const TopYoutube = () => {\n      fetch(\"https://raw.githubusercontent.com/Kim-chanmi/react_api/main/src/utils/youtube_video.json\").then(response => response.json()).then(result => sliders.value = result.items)\n      // .then((result) => console.log(result))\n      .catch(error => console.log(\"error\", error));\n    };\n    TopYoutube();\n    const SearchYoutubes = () => {\n      fetch(`https://youtube.googleapis.com/youtube/v3/search?key=AIzaSyBMw5RG0lHjQd8xTnmA63HCsEe5tXuQ764&part=snippet&maxResults=10&type=video&q=${search.value}`).then(response => response.json()).then(result => {\n        youtubes.value = result.items;\n        search.value = \"\";\n      })\n      // .then((result) => console.log(result))\n      .catch(error => console.log(\"error\", error));\n    };\n    SearchYoutubes();\n    return {\n      modules: [EffectCreative],\n      youtubes,\n      sliders,\n      search,\n      RandomYoutube,\n      TopYoutube,\n      SearchYoutubes\n    };\n  }\n};","map":{"version":3,"mappings":"AA6EA,OAAOA,UAAS,MAAO,6BAA6B;AACpD,OAAOC,UAAS,MAAO,6BAA6B;AACpD,OAAOC,SAAQ,MAAO,4BAA4B;AAClD,OAAOC,WAAU,MAAO,8BAA8B;AACtD,SAASC,GAAE,QAAS,KAAK;AAEzB,SAASC,MAAM,EAAEC,WAAU,QAAS,YAAY;AAChD,OAAO,YAAY;AACnB,OAAO,4BAA4B;AACnC,SAASC,cAAa,QAAS,QAAQ;AAEvC,eAAe;EACbC,UAAU,EAAE;IACVR,UAAU;IACVC,UAAU;IACVC,SAAS;IACTC,WAAW;IACXE,MAAM;IACNC;EACF,CAAC;EAEDG,KAAK,GAAG;IACN,MAAMC,QAAO,GAAIN,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMO,OAAM,GAAIP,GAAG,CAAC,EAAE,CAAC;IACvB,MAAMQ,MAAK,GAAIR,GAAG,CAAC,WAAW,CAAC;IAE/B,MAAMS,aAAY,GAAI,MAAM;MAC1BC,KAAK,CACH,0FAAyF,CAC3F,CACGC,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACC,IAAI,EAAE,EAClCF,IAAI,CAAEG,MAAM,IAAMR,QAAQ,CAACS,KAAI,GAAID,MAAM,CAACE,KAAM;MACjD;MAAA,CACCC,KAAK,CAAEC,KAAK,IAAKC,OAAO,CAACC,GAAG,CAAC,OAAO,EAAEF,KAAK,CAAC,CAAC;IAClD,CAAC;IACDT,aAAa,EAAE;IAEf,MAAMY,UAAS,GAAI,MAAM;MACvBX,KAAK,CACH,0FAAyF,CAC3F,CACGC,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACC,IAAI,EAAE,EAClCF,IAAI,CAAEG,MAAM,IAAMP,OAAO,CAACQ,KAAI,GAAID,MAAM,CAACE,KAAM;MAChD;MAAA,CACCC,KAAK,CAAEC,KAAK,IAAKC,OAAO,CAACC,GAAG,CAAC,OAAO,EAAEF,KAAK,CAAC,CAAC;IAClD,CAAC;IACDG,UAAU,EAAE;IAEZ,MAAMC,cAAa,GAAI,MAAM;MAC3BZ,KAAK,CACF,wIAAuIF,MAAM,CAACO,KAAM,GACvJ,CACGJ,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACC,IAAI,EAAE,EAClCF,IAAI,CAAEG,MAAM,IAAK;QAChBR,QAAQ,CAACS,KAAI,GAAID,MAAM,CAACE,KAAK;QAC7BR,MAAM,CAACO,KAAI,GAAI,EAAE;MACnB,CAAC;MACD;MAAA,CACCE,KAAK,CAAEC,KAAK,IAAKC,OAAO,CAACC,GAAG,CAAC,OAAO,EAAEF,KAAK,CAAC,CAAC;IAClD,CAAC;IACDI,cAAc,EAAE;IAEhB,OAAO;MACLC,OAAO,EAAE,CAACpB,cAAc,CAAC;MACzBG,QAAQ;MACRC,OAAO;MACPC,MAAM;MACNC,aAAa;MACbY,UAAU;MACVC;IACF,CAAC;EACH;AACF,CAAC","names":["HeaderCont","FooterCont","TitleCont","ContactCont","ref","Swiper","SwiperSlide","EffectCreative","components","setup","youtubes","sliders","search","RandomYoutube","fetch","then","response","json","result","value","items","catch","error","console","log","TopYoutube","SearchYoutubes","modules"],"sourceRoot":"","sources":["C:\\Users\\chanm\\Documents\\vue_api\\src\\views\\YoutubeView.vue"],"sourcesContent":["<template>\n  <div>\n    <HeaderCont />\n    <TitleCont name1=\"youtube\" name2=\"reference api\" />\n    <section class=\"youtube__wrap\">\n      <div class=\"container\">\n        <div class=\"youtube__popular\">\n          <div class=\"youtube__inner\">\n            <h2>Youtube Random</h2>\n            <li>\n              <swiper\n                :grabCursor=\"true\"\n                :effect=\"'creative'\"\n                :creativeEffect=\"{\n                  prev: {\n                    shadow: false,\n                    translate: [0, 0, -400],\n                  },\n                  next: {\n                    translate: ['100%', 0, 0],\n                  },\n                }\"\n                :modules=\"modules\"\n                class=\"mySwiper\"\n              >\n                <swiper-slide\n                  v-for=\"slider in sliders\"\n                  :key=\"slider.id.videoId\"\n                >\n                  <a\n                    :href=\"`https://www.youtube.com/watch?v=${slider.id.videoId}`\"\n                  >\n                    <img\n                      :src=\"slider.snippet.thumbnails.medium.url\"\n                      :alt=\"slider.snippet.description\"\n                    />\n                  </a>\n                </swiper-slide>\n              </swiper>\n            </li>\n          </div>\n        </div>\n        <div class=\"movie__search\">\n          <h2>검색하기</h2>\n          <form @submit.prevent=\"SearchYoutubes()\">\n            <input\n              type=\"search\"\n              id=\"search\"\n              placeholder=\"검색하세요\"\n              v-model=\"search\"\n            />\n            <button type=\"submit\">검색</button>\n          </form>\n        </div>\n        <div class=\"youtube__inner\">\n          <ul>\n            <li v-for=\"youtube in youtubes\" :key=\"youtube.id.videoId\">\n              <a\n                :href=\"`https://www.youtube.com/watch?v=${youtube.id.videoId}`\"\n              >\n                <img\n                  :src=\"youtube.snippet.thumbnails.medium.url\"\n                  :alt=\"youtube.snippet.description\"\n                />\n                <span>{{ youtube.snippet.title }}</span>\n              </a>\n            </li>\n          </ul>\n        </div>\n      </div>\n    </section>\n    <ContactCont />\n    <FooterCont />\n  </div>\n</template>\n\n<script>\nimport HeaderCont from \"@/components/HeaderCont.vue\";\nimport FooterCont from \"@/components/FooterCont.vue\";\nimport TitleCont from \"@/components/TitleCont.vue\";\nimport ContactCont from \"@/components/ContactCont.vue\";\nimport { ref } from \"vue\";\n\nimport { Swiper, SwiperSlide } from \"swiper/vue\";\nimport \"swiper/css\";\nimport \"swiper/css/effect-creative\";\nimport { EffectCreative } from \"swiper\";\n\nexport default {\n  components: {\n    HeaderCont,\n    FooterCont,\n    TitleCont,\n    ContactCont,\n    Swiper,\n    SwiperSlide,\n  },\n\n  setup() {\n    const youtubes = ref([]);\n    const sliders = ref([]);\n    const search = ref(\"landscape\");\n\n    const RandomYoutube = () => {\n      fetch(\n        \"https://raw.githubusercontent.com/Kim-chanmi/react_api/main/src/utils/youtube_video.json\"\n      )\n        .then((response) => response.json())\n        .then((result) => (youtubes.value = result.items))\n        // .then((result) => console.log(result))\n        .catch((error) => console.log(\"error\", error));\n    };\n    RandomYoutube();\n\n    const TopYoutube = () => {\n      fetch(\n        \"https://raw.githubusercontent.com/Kim-chanmi/react_api/main/src/utils/youtube_video.json\"\n      )\n        .then((response) => response.json())\n        .then((result) => (sliders.value = result.items))\n        // .then((result) => console.log(result))\n        .catch((error) => console.log(\"error\", error));\n    };\n    TopYoutube();\n\n    const SearchYoutubes = () => {\n      fetch(\n        `https://youtube.googleapis.com/youtube/v3/search?key=AIzaSyBMw5RG0lHjQd8xTnmA63HCsEe5tXuQ764&part=snippet&maxResults=10&type=video&q=${search.value}`\n      )\n        .then((response) => response.json())\n        .then((result) => {\n          youtubes.value = result.items;\n          search.value = \"\";\n        })\n        // .then((result) => console.log(result))\n        .catch((error) => console.log(\"error\", error));\n    };\n    SearchYoutubes();\n\n    return {\n      modules: [EffectCreative],\n      youtubes,\n      sliders,\n      search,\n      RandomYoutube,\n      TopYoutube,\n      SearchYoutubes,\n    };\n  },\n};\n</script>\n<style lang=\"scss\">\n.youtube__inner {\n  ul {\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: flex-start;\n    gap: 10px;\n    font-family: var(--font-kor1);\n    li {\n      width: 24%;\n      margin-bottom: 20px;\n\n      span {\n        display: inline-block;\n        padding: 5px 0;\n        white-space: nowrap;\n        overflow: hidden;\n        text-overflow: ellipsis;\n        width: 100%;\n      }\n      img {\n        border-radius: 5px;\n        overflow: hidden;\n      }\n    }\n  }\n}\n.youtube__inner {\n  h2 {\n    font-size: 40px;\n    color: var(--black);\n  }\n  li {\n    list-style: none;\n  }\n  .swiper img {\n    margin: 100px auto;\n  }\n\n  .swiper-slide {\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    font-size: 22px;\n    font-weight: bold;\n    background: none !important;\n  }\n}\n.youtube__popular {\n  ul {\n    column-count: 4;\n    column-gap: 20px;\n    width: 100%;\n\n    li {\n      margin-bottom: 20px;\n\n      img {\n        border-radius: 5px;\n      }\n    }\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}