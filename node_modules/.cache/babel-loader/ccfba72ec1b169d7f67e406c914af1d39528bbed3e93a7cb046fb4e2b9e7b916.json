{"ast":null,"code":"import HeaderCont from \"@/components/HeaderCont.vue\";\nimport FooterCont from \"@/components/FooterCont.vue\";\nimport TitleCont from \"@/components/TitleCont.vue\";\nimport ContactCont from \"@/components/ContactCont.vue\";\nimport { ref } from \"vue\";\nexport default {\n  components: {\n    HeaderCont,\n    TitleCont,\n    ContactCont,\n    FooterCont\n  },\n  setup() {\n    const movies = ref([]);\n    const sliders = ref([]);\n    const search = ref(\"marvel\");\n    const SearchMovies = async () => {\n      fetch(`https://api.themoviedb.org/3/search/movie?api_key=130fd98d6e6907020a62f2869eed23e1&query=${search.value}`).then(response => response.json()).then(result => {\n        movies.value = result.results;\n        search.value = \"\";\n      }).catch(error => console.log(error));\n    };\n    SearchMovies();\n    const TopMoviies = async () => {\n      fetch(`https://api.themoviedb.org/3/movie/popular?api_key=130fd98d6e6907020a62f2869eed23e1&language=en-US`).then(response => response.json()).then(result => sliders.value = result.results).catch(error => console.log(error));\n    };\n    TopMoviies();\n    return {\n      movies,\n      sliders,\n      search,\n      SearchMovies\n    };\n  }\n};","map":{"version":3,"mappings":"AA2CA,OAAOA,UAAS,MAAO,6BAA6B;AACpD,OAAOC,UAAS,MAAO,6BAA6B;AACpD,OAAOC,SAAQ,MAAO,4BAA4B;AAClD,OAAOC,WAAU,MAAO,8BAA8B;AACtD,SAASC,GAAE,QAAS,KAAK;AAEzB,eAAe;EACbC,UAAU,EAAE;IACVL,UAAU;IACVE,SAAS;IACTC,WAAW;IACXF;EACF,CAAC;EACDK,KAAK,GAAG;IACN,MAAMC,MAAK,GAAIH,GAAG,CAAC,EAAE,CAAC;IACtB,MAAMI,OAAM,GAAIJ,GAAG,CAAC,EAAE,CAAC;IACvB,MAAMK,MAAK,GAAIL,GAAG,CAAC,QAAQ,CAAC;IAE5B,MAAMM,YAAW,GAAI,YAAY;MAC/BC,KAAK,CACF,4FAA2FF,MAAM,CAACG,KAAM,GAC3G,CACGC,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACC,IAAI,EAAE,EAClCF,IAAI,CAAEG,MAAM,IAAK;QAChBT,MAAM,CAACK,KAAI,GAAII,MAAM,CAACC,OAAO;QAC7BR,MAAM,CAACG,KAAI,GAAI,EAAE;MACnB,CAAC,EACAM,KAAK,CAAEC,KAAK,IAAKC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC,CAAC;IACzC,CAAC;IACDT,YAAY,EAAE;IAEd,MAAMY,UAAS,GAAI,YAAY;MAC7BX,KAAK,CACF,oGAAkG,CACrG,CACGE,IAAI,CAAEC,QAAQ,IAAKA,QAAQ,CAACC,IAAI,EAAE,EAClCF,IAAI,CAAEG,MAAM,IAAMR,OAAO,CAACI,KAAI,GAAII,MAAM,CAACC,OAAQ,EACjDC,KAAK,CAAEC,KAAK,IAAKC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC,CAAC;IACzC,CAAC;IACDG,UAAU,EAAE;IACZ,OAAO;MAAEf,MAAM;MAAEC,OAAO;MAAEC,MAAM;MAAEC;IAAa,CAAC;EAClD;AACF,CAAC","names":["HeaderCont","FooterCont","TitleCont","ContactCont","ref","components","setup","movies","sliders","search","SearchMovies","fetch","value","then","response","json","result","results","catch","error","console","log","TopMoviies"],"sourceRoot":"","sources":["C:\\Users\\chanm\\Documents\\vue_api\\src\\views\\MovieView.vue"],"sourcesContent":["<template>\n  <div>\n    <HeaderCont />\n    <TitleCont name1=\"movie\" name2=\"reference api\" />\n    <section class=\"cont_movie\">\n      <div class=\"container\">\n        <div class=\"movie__slider\"></div>\n        <div class=\"movie__search\">\n          <h2>검색하기</h2>\n          <form @submit.prevent=\"SearchMovies()\">\n            <input\n              type=\"search\"\n              id=\"search\"\n              placeholder=\"검색하세요\"\n              v-model=\"search\"\n            />\n            <button type=\"submit\">검색</button>\n          </form>\n        </div>\n        <div class=\"movie__inner\">\n          <ul>\n            <li v-for=\"movie in movies\" :key=\"movie.id\">\n              <a :href=\"`https://www.themoviedb.org/movie/${movie.id}`\">\n                <img\n                  :src=\"`https://image.tmdb.org/t/p/w500/${movie.poster_path}`\"\n                  :alt=\"movie.title\"\n                />\n                <em>\n                  <span class=\"title\">{{ movie.title }}</span>\n                  <span class=\"star\">{{ movie.vote_average }}</span>\n                </em>\n              </a>\n            </li>\n          </ul>\n        </div>\n      </div>\n    </section>\n    <ContactCont />\n    <FooterCont />\n  </div>\n</template>\n\n<script>\nimport HeaderCont from \"@/components/HeaderCont.vue\";\nimport FooterCont from \"@/components/FooterCont.vue\";\nimport TitleCont from \"@/components/TitleCont.vue\";\nimport ContactCont from \"@/components/ContactCont.vue\";\nimport { ref } from \"vue\";\n\nexport default {\n  components: {\n    HeaderCont,\n    TitleCont,\n    ContactCont,\n    FooterCont,\n  },\n  setup() {\n    const movies = ref([]);\n    const sliders = ref([]);\n    const search = ref(\"marvel\");\n\n    const SearchMovies = async () => {\n      fetch(\n        `https://api.themoviedb.org/3/search/movie?api_key=130fd98d6e6907020a62f2869eed23e1&query=${search.value}`\n      )\n        .then((response) => response.json())\n        .then((result) => {\n          movies.value = result.results;\n          search.value = \"\";\n        })\n        .catch((error) => console.log(error));\n    };\n    SearchMovies();\n\n    const TopMoviies = async () => {\n      fetch(\n        `https://api.themoviedb.org/3/movie/popular?api_key=130fd98d6e6907020a62f2869eed23e1&language=en-US`\n      )\n        .then((response) => response.json())\n        .then((result) => (sliders.value = result.results))\n        .catch((error) => console.log(error));\n    };\n    TopMoviies();\n    return { movies, sliders, search, SearchMovies };\n  },\n};\n</script>\n<style lang=\"scss\">\n.movie__inner {\n  ul {\n    display: flex;\n    justify-content: space-between;\n    flex-wrap: wrap;\n\n    li {\n      width: 23%;\n      position: relative;\n      transition: all 0.3s;\n\n      &:hover {\n        scale: 1.05;\n      }\n\n      em {\n        display: block;\n        height: 80px;\n        margin-bottom: 30px;\n        font-family: var(--font-kor1);\n      }\n\n      .title {\n        padding: 5px 0;\n        display: inline-block;\n        color: #000;\n      }\n      .star {\n        background: #fff;\n        color: #000;\n        position: absolute;\n        left: 10px;\n        top: 10px;\n        width: 30px;\n        height: 30px;\n        border-radius: 50%;\n        text-align: center;\n        line-height: 30px;\n        font-weight: 800;\n      }\n    }\n  }\n  a {\n    color: var(--white);\n  }\n}\n\n.movie__search {\n  margin-bottom: 100px;\n  position: relative;\n\n  .container {\n    position: relative;\n  }\n\n  h2 {\n    color: var(--black);\n    font-size: 40px;\n    text-indent: -9999px;\n    height: 0;\n  }\n\n  input {\n    background: #d5d5d5;\n    border: 1px solid #d5d5d5;\n    border-radius: 50px;\n    color: var(--black);\n    width: 100%;\n    padding: 14px 30px;\n    font-family: var(--font-kor1);\n  }\n  button {\n    position: absolute;\n    right: 8px;\n    top: 5px;\n    width: 40px;\n    height: 40px;\n    border-radius: 50%;\n    border: 0;\n    font-family: var(--font-kor1);\n    cursor: pointer;\n    z-index: 1000;\n    color: #000;\n  }\n}\n.movie__pop {\n  margin-top: 150px;\n  width: 100%;\n  overflow-x: auto;\n\n  .movie__popular_inner {\n    color: var(--black);\n    font-size: 25px;\n    margin-bottom: 70px;\n    text-align: left;\n    font-family: var(--font-kor1);\n  }\n\n  ul {\n    display: flex;\n    justify-content: space-between;\n    width: 80%;\n    margin: 0 auto;\n\n    .swiper-wrapper {\n      width: 30%;\n\n      .swiper-slide-active img {\n        border: 4px solid #fff;\n        filter: brightness(140%);\n      }\n    }\n\n    li {\n      width: 65%;\n      position: relative;\n      transition: all 0.3s;\n      margin-bottom: 60px;\n\n      &:hover {\n        scale: 1.05;\n      }\n    }\n    em {\n      display: block;\n      height: 80px;\n      margin-bottom: 30px;\n      font-family: var(--font-kor);\n    }\n    .title {\n      padding: 5px 0;\n      display: inline-block;\n    }\n    .star {\n      background: #fff;\n      color: #000;\n      position: absolute;\n      left: 10px;\n      top: 10px;\n      width: 30px;\n      height: 30px;\n      border-radius: 50%;\n      text-align: center;\n      line-height: 30px;\n      font-weight: 700;\n    }\n  }\n  a {\n    color: var(--black);\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}